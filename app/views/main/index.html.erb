<% provide(:Votacion, "active") %>
<% provide(:Title, "Votación") %>

<div class="panel panel-default">
  	<div class="panel-heading">
    	<h3 class="panel-title">Busqueda de puestos de votación para asociación</h3>
  	</div>
  	<div class="panel-body">
	    <%= form_tag "/search/create", method: :get, remote:  true, id:"search-form" do %>
			<%= text_field_tag :keyword, nil, placeholder: "Buscar puesto" %>
			<button type="submit" class="btn btn-default">Buscar</button>
		<%end%>

		<div id="resultados">
		</div>
  	</div>
</div>